        baidu.dom.ready(function(){
            try {
                var loaded = false;

                var getDataUrl = "/static/absolute/travel-book/data.js?t=" + (new Date()).getTime();
                baidu.ajax.get(getDataUrl, function(r){
                    var contents = baidu.json.parse(r.responseText);
                    var sid =  baidu.lang.getModule("lv.scene.view.sid");
                    baidu.array.each(contents, function(content, i){
                        var pairs = content["pairs"];
                        var realSid = content["sid"];
                        baidu.array.each(pairs, function(pair){
                            if(pair.sid == sid && !loaded){
                                //render
                                var tpl = baidu.dom.g("travel-book-tpl");
                                var html = baidu.template(tpl.value, content);
                                var sidebarBasic= baidu.dom.query('section.sidebar-basic')[0];
                                if ( !!sidebarBasic ) {
                                    var article = baidu.dom.create('div', {
                                        'class': "sidebar-mod"
                                    });
                                    article.innerHTML = html;
                                    sidebarBasic.parentNode.insertBefore(article, sidebarBasic);
                                    loaded = true;
                                    var getTotalUrl = "/scene/ajax/downbook?sid=" + realSid;
                                    baidu.ajax.get(getTotalUrl + '&t='
                                            + new Date().getTime(), function(res){
                                        var json = baidu.json.parse(res.responseText);
                                        var total = json.data.total;
                                        baidu.dom.g("J_download-count").innerHTML = total;
                                    });
baidu.event.on('travel-book-download-btn', 'click', function(){   lv.nslog(window.location.href,306,{"page":"view","cmd":"click","pos":"download-click"});});
baidu.event.on('travel-book-img', 'click', function(){   lv.nslog(window.location.href,306,{"page":"view","cmd":"click","pos":"download-click"});});
                                lv.nslog(window.location.href,306,{"page":"scene","cmd":"show","pos":"travel-book-view-count"});
                                } else {
                                    var test = baidu.dom.query('.col-sub')[0];
                                    var article = baidu.dom.create('div', {
                                        'class': "sidebar-mod"
                                    });
                                    article.innerHTML = html;
                                    baidu.dom.insertBefore( article, baidu.dom.first( test ) );
                                    loaded = true;
                                    var getTotalUrl = "/scene/ajax/downbook?sid=" + realSid;
                                    baidu.ajax.get(getTotalUrl + '&t='
                                            + new Date().getTime(), function(res){
                                        var json = baidu.json.parse(res.responseText);
                                        var total = json.data.total;
                                        baidu.dom.g("J_download-count").innerHTML = total;

                                    });
baidu.event.on('travel-book-download-btn', 'click', function(){   lv.nslog(window.location.href,306,{"page":"view","cmd":"click","pos":"download-click"});});
baidu.event.on('travel-book-img', 'click', function(){   lv.nslog(window.location.href,306,{"page":"view","cmd":"click","pos":"download-click"});});
                                lv.nslog(window.location.href,306,{"page":"scene","cmd":"show","pos":"travel-book-view-count"});
                                }
                            }
                        });
                    });
                });
            }
            catch (e){
                cosnole.log(e);
            }
        });
    </script>

